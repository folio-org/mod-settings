{
  "description": "Tenant address entry",
  "type": "object",
  "properties": {
    "id": {
      "description": "Identifier",
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "description": "Display name for the address",
      "type": "string"
    },
    "address": {
      "description": "Address text",
      "type": "string"
    },
    "metadata": {
      "type": "object",
      "description": "Metadata containing user and date information",
      "$ref": "metadata.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "address"
  ]
}
